	<h2>Download Movies:</h2>
    <form id="gettorrent" action="/torrent" method="POST" enctype="multipart/form-data">
        Insert .torrent file:
        <br>
        <input type="file" name="torrentfile" id="torrentfile">
        <br>
        <input type="submit" value="Uploud">
    </form>


    <h2>Available movies:</h2>
    <% for( var i = 0; i<movies.amount; i++ ){ %>
        <li><a href="/cinema/<%=movies.library[i].name%>"> <%=movies.library[i].name%> </a></li>
    <% } %>
    
